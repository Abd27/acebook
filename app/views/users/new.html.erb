<h1>Sign Up </h1>
<%= form_for @user do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :dob %>
  <%= form.date_field :dob %>
  <%= form.label :email %>
  <%= form.email_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.label :confirm_password %>
  <%= form.password_field :confirm_password %>
  

  <%= form.submit "Submit" %>
<% end %>

